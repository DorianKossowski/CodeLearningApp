### TASK ###

### INPUT ###
public class MyClass {
    private static int counter = 0;

    public static void main(String[] args){
        fun();
    }

    static void fun() {
        for(int i=0; i<3; i = i+1) {
            System.out.println(i*2);
            if(i == 1) {
                return;
            }
        }

        while(true) {
            System.out.println(i+2);
        }
    }
}

### OUTPUT ###
0
2
