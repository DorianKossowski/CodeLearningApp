public class MyClass {
    public static void main(String[] args){
        int i = 98;
        char c = 'b';

        boolean result = i == c;    // true

        double a = 10.0;
        int b = 1;
        boolean b1 = a >= b;    // true
        a = -1;
        boolean b2 = a*2 < b+1; // true
        a = 3;
