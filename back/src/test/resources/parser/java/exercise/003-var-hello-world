### TASK ###
-> method
	with modifiers: { "public", "static" }
	with result: "void"
	with name: "main"
	with args: { "String[]", - }
-> statement
	in method: "main"
	with text: "System.out.print(message);"
	with resolved: "System.out.print(\"Hello World\");"
	error message: "Wywołanie metody z użyciem zmiennej."

### INPUT ###
public class Hello {
    public static void main(String[] args){
        String message = "Hello World";
        System.out.print(message);
    }
}